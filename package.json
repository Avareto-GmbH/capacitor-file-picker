{
  "name": "capacitor-file-picker",
  "version": "0.0.1",
  "description": "This plugin presents the native UI for picking a file.",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Jose Martinez",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^2.4.6"
  },
  "devDependencies": {
    "@capacitor/android": "^2.4.6",
    "@capacitor/ios": "^2.4.6",
    "rimraf": "^3.0.0",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorFilePicker.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epicshaggy/capaitor-file-picker"
  },
  "bugs": {
    "url": "https://github.com/epicshaggy/capaitor-file-picker/issues"
  }
